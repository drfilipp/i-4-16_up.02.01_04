<Window x:Class="HealthCenter.SplashScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HealthCenter"
        mc:Ignorable="d"
        Title="MainWindow" Height="250" Width="409" WindowStyle="None" WindowStartupLocation="CenterScreen" Margin="0,0,0,100"
        Loaded="Window_Loaded">
    <Window.Background>
        <ImageBrush ImageSource="SplashScreen1.png"/>
    </Window.Background>
    <Grid>
        <ProgressBar x:Name="bar" Value="0" Maximum="100" Margin="20,198,21,27" />
    </Grid>
</Window>
